# Quasar Frontend Environment Variables

# API Configuration
VUE_APP_API_BASE_URL=http://localhost:5000/api/v1

# Port Configuration
# Backend port for API requests (used by ports.ts)
VITE_BACKEND_PORT=5000

# Application Configuration
VUE_APP_TITLE=Hydroponics System
VUE_APP_VERSION=1.0.0

# Development Settings
VUE_APP_DEBUG=true
VUE_APP_LOG_LEVEL=debug

# Router Configuration
VUE_ROUTER_MODE=history
VUE_ROUTER_BASE=/

# Build Configuration
NODE_ENV=development