{
    "id": "arduino_uno_r4_wifi",
    "name": "Arduino Uno R4 WiFi",
    "description": "Renesas RA4M1 32-bit MCU with ESP32-S3 WiFi Module",
    "architecture": "renesas_uno",
    "voltage": 5.0,
    "clock_speed_hz": 48000000,
    "memory": {
        "flash_bytes": 262144,
        "sram_bytes": 32768,
        "eeprom_bytes": 8192
    },
    "interfaces": {
        "serial": {
            "hardware": [
                "Serial",
                "Serial1"
            ],
            "usb": "Serial"
        },
        "i2c": [
            "Wire"
        ],
        "spi": [
            "SPI"
        ],
        "wifi": {
            "type": "native",
            "chipset": "ESP32-S3"
        }
    },
    "pins": {
        "digital_count": 14,
        "analog_input_count": 6,
        "analog_output_count": 1,
        "pwm_pins": [
            3,
            5,
            6,
            9,
            10,
            11
        ],
        "mappings": {
            "LED_BUILTIN": 13,
            "SDA": "SDA",
            "SCL": "SCL"
        }
    },
    "requirements": {
        "core": "arduino:renesas_uno",
        "libraries": [
            {
                "name": "WiFiS3",
                "version": "^0.0.0",
                "required_for": [
                    "wifi"
                ]
            }
        ]
    },
    "defines": [
        "ARDUINO_UNOR4_WIFI"
    ]
}