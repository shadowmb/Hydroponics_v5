{
    "id": "arduino_uno_r3",
    "name": "Arduino Uno R3",
    "description": "Classic ATmega328P based board",
    "architecture": "avr",
    "voltage": 5.0,
    "clock_speed_hz": 16000000,
    "memory": {
        "flash_bytes": 32256,
        "sram_bytes": 2048,
        "eeprom_bytes": 1024
    },
    "interfaces": {
        "serial": {
            "hardware": [
                "Serial"
            ],
            "usb": "Serial"
        },
        "i2c": [
            "Wire"
        ],
        "spi": [
            "SPI"
        ],
        "wifi": {
            "type": "none",
            "chipset": null
        }
    },
    "pins": {
        "digital_count": 14,
        "analog_input_count": 6,
        "analog_output_count": 0,
        "pwm_pins": [
            3,
            5,
            6,
            9,
            10,
            11
        ],
        "mappings": {
            "LED_BUILTIN": 13,
            "SDA": "A4",
            "SCL": "A5"
        }
    },
    "requirements": {
        "core": "arduino:avr",
        "libraries": []
    },
    "defines": [
        "ARDUINO_UNO"
    ]
}